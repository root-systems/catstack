{
  "name": "business-stack",
  "version": "0.0.0",
  "description": "real-world production-quality TodoMVC example",
  "main": "server",
  "scripts": {
    "postinstall": "lnfs app node_modules/app",
    "lint": "snazzy",
    "format": "snazzy --format",
    "test": "npm-run-all -p test:*",
    "test:spec": "node app/spec",
    "test:feature": "node app/features",
    "dev:client": "BABEL_ENV=hot watchify client -o build/bundle.js -dv -p browserify-hmr",
    "dev:assets": "cpx \"app/assets/**/*\" build -w",
    "dev:server": "node-dev server",
    "prod:client": "browserify client -o build/bundle.js -g envify -g uglifyify",
    "prod:assets": "cpx \"app/assets/**/*\" build",
    "prod:server": "node server",
    "dev": "NODE_ENV=development npm-run-all -p dev:*",
    "prod": "NODE_ENV=production npm-run-all -s prod:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enspiral-craftworks/business-stack.git"
  },
  "keywords": [
    "todomvc",
    "example",
    "react",
    "redux",
    "frp",
    "functional",
    "reactive",
    "immutable",
    "feathers",
    "ddd",
    "tcomb"
  ],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/enspiral-craftworks/business-stack/issues"
  },
  "homepage": "https://github.com/enspiral-craftworks/business-stack#readme",
  "engines": {
    "node": "^4.0.0",
    "npm": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.8.0",
    "babel-plugin-react-transform": "^2.0.0",
    "browserify-hmr": "^0.3.1",
    "cuke-tap": "^1.0.2",
    "glob": "^6.0.2",
    "jsdom": "^7.1.0",
    "node-dev": "^2.7.1",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "redux-logger": "^2.0.4",
    "run-parallel": "^1.1.4",
    "tape": "^4.4.0",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "babel-core": "^6.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.1.18",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-runtime": "^6.2.0",
    "babelify": "^7.2.0",
    "browserify": "github:ahdinosaur/node-browserify",
    "bulk-require": "^0.2.1",
    "bulkify": "^1.1.1",
    "cpx": "^1.2.1",
    "css-modules-require-hook": "^2.1.0",
    "cssify": "github:ahdinosaur/cssify",
    "envify": "^3.4.0",
    "evalify": "^1.0.1",
    "feathers": "^1.2.0",
    "history": "^1.13.1",
    "lnfs-cli": "^1.0.1",
    "npm-run-all": "^1.3.2",
    "pinkie-promise": "^2.0.0",
    "ramda": "^0.18.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.1",
    "react-router": "^1.0.0",
    "redux": "^3.0.5",
    "redux-simple-router": "0.0.10",
    "redux-thunk": "^1.0.0",
    "serve-static": "^1.10.0",
    "uglifyify": "^3.0.1"
  }
}
